<p id="notice"><%= notice %></p>

<h1>Computers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Active</th>
      <th>Available</th>
      <th>Owner</th>
      <th>Manufacturer</th>
      <th>Model</th>
      <th>Serial</th>
      <th>Product</th>
      <th>Space</th>
      <th>Processor</th>
      <th>Ram</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @computers.each do |computer| %>
      <tr>
        <td><%= computer.name %></td>
        <td><%= computer.active %></td>
        <td><%= computer.available %></td>
        <td><%= computer.owner %></td>
        <td><%= computer.manufacturer %></td>
        <td><%= computer.model %></td>
        <td><%= computer.serial %></td>
        <td><%= computer.product %></td>
        <td><%= computer.space %></td>
        <td><%= computer.processor %></td>
        <td><%= computer.ram %></td>
        <td><%= link_to 'Show', computer %></td>
        <td><%= link_to 'Edit', edit_computer_path(computer) %></td>
        <td><%= link_to 'Destroy', computer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Computer', new_computer_path %>
